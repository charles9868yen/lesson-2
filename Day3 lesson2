def main():
  import random  
  num=(random.randint(1,10))
  print("Guess a number, 1 to 10")
  x=int(input())
  while x!=num:
    print("Your Wrong!!!!!!")
    print("Guess IT again")
    x=int(input())

  print("Your Right!!! It's: ", num)
  print("Play again??? Yes=0 No= else")
  y=int(input())
  if y==0:
    main()
  else:
    print("byebye!")
main()
